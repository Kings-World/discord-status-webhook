{
    "problemMatcher": [
        {
            "owner": "tsc",
            "pattern": [
                {
                    "regexp": "^([^\\s].*)[\\(:](\\d+)[,:](\\d+)(?:\\):\\s+|\\s+-\\s+)(error|warning|info)\\s+TS(\\d+)\\s*:\\s*((?:.|\n)*)",
                    "file": 1,
                    "location": 2,
                    "severity": 3,
                    "code": 4,
                    "message": 5
                }
            ]
        }
    ]
}
